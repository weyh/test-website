{
  "template": {
    "types": {
      "channels": {
        "name": "Youtube Channels:",
        "boilerplate": "<div class='row animated fadeIn fast scale'><div class='col-6 col-sm-1' onclick=\"openInNewTab('${channelUrl}')\" style='text-align: center; cursor: pointer;'> <img src='${imgUrl}' alt='${name}' height='46em' /> </div> <div class='col'> <div class='row' style='padding-left: 1em !important; cursor: pointer;' onclick=\"openInNewTab('${channelUrl}')\"> <div class='col-sm'><a href='javascript:;'>${name}</a></div> <div class='w-100 d-none d-md-block'></div> <div class='col' id='moby'>${description}</div> </div> </div> </div> </div>"
      },
      "playlists": {
        "name": "Playlists:",
        "boilerplate": "<li class='animated fadeIn fast scaleL'><a href='${url}' target='_blank'>${name}</a></li>"
      },
      "sites": {
        "name": "Sites:",
        "boilerplate": "<li class='animated fadeIn fast scaleL'><a href='${url}' target='_blank'>${name}</a></li>"
      },
      "tools": {
        "name": "Programs:",
        "boilerplate": "<li class='animated fadeIn fast scaleL'> <a href='${url}' target='_blank'>${name}</a> <div style='margin-left: 1.4em'${description}</div> </li>"
      },
      "myCodeExtensions": {
        "name": "Visual Studio Code Extensions:",
        "boilerplate": "<li class='animated fadeIn fast scaleL'> <a href='${url}' target='_blank'>${name}</a></li>"
      }
    },
    "spacer": "<div class='w-100 d-none d-md-block' style='margin-top: 0.5em'></div>"
  },

